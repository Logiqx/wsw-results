{
  "Periods": [
    {
      "Suptitle": "Day Rankings by Status - {Date}",
      "Period": "Session"
    },
    {
      "Suptitle": "Overall Rankings by Status",
      "Period": "Event",
      "HideFields": "Course,Time"
    }
  ],
  "Profile": true,
  "Reports": [
    {
      "Title": "All Known Pro Sailboards",
      "Comment": "Note: Potentially inaccurate due to unclassified sailboards (see below).",
      "Fields": "Name,Nation,Sail Number,Course,Time,Speed (kts)",
      "Filter": "Craft Type=Sailboard,Status=Pro Fleet"
    },
    {
      "Title": "All Known Amateur Sailboards",
      "Comment": "Note: Potentially inaccurate due to unclassified sailboards (see below).",
      "Fields": "Name,Nation,Sail Number,Course,Time,Speed (kts)",
      "Filter": "Craft Type=Sailboard,Status=Amateur"
    },
    {
      "Title": "All Unclassified Sailboards",
      "Comment": "Note: These all belong in 'Pro Sailboards' or 'Amateur Sailboards' (see above).",
      "Fields": "Name,Nation,Sail Number,Status,Course,Time,Speed (kts)",
      "Filter": "Craft Type=Sailboard,Status=TBC"
    },
    {
      "Title": "Top Novice Sailboards",
      "Fields": "Name,Nation,Sail Number,Status,Course,Time,Speed (kts)",
      "Filter": "Craft Type=Sailboard,Novice=Y",
      "Period": "Event"
    },
    {
      "Title": "All Lady Sailboards",
      "Fields": "Name,Nation,Sail Number,Status,Course,Time,Speed (kts)",
      "Filter": "Craft Type=Sailboard,Gender=F"
    },
    {
      "Title": "All Youths",
      "Comment": "(age <17 on the first day of speed week)",
      "Fields": "Name,Nation,Sail Number,Craft Type,Course,Time,Speed (kts)",
      "Filter": "Status=Youth Fleet",
      "Period": "Session"
    },
    {
      "Title": "All Youths",
      "Comment": "(age <17 on the first day of speed week)",
      "Fields": "Name,Nation,Sail Number,Craft Type,Course,Time,Speed (kts)",
      "Filter": "Status=Youth Fleet,Weekend=Y",
      "Sort": "Speed (kts)=DSC",
      "Period": "Event"
    },
    {
      "Title": "All Known Master Sailboards",
      "Fields": "Name,Nation,Sail Number,Status,Course,Time,Speed (kts)",
      "Comment": "(age 45+)",
      "Filter": "Craft Type=Sailboard,Master=Y"
    }
  ]
}
