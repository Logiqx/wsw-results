{
  "Periods": [
    {
      "Suptitle": "Day Rankings by Status - {Date}",
      "Period": "Session"
    },
    {
      "Suptitle": "Overall Rankings by Status",
      "Period": "Event",
      "HideFields": "Course,Start Time"
    }
  ],
  "Profile": true,
  "Reports": [
    {
      "Title": "All Ladies",
      "Fields": "Name,Nation,Craft Type,Status,Course,Start Time,Speed (kts)",
      "Filter": "Gender=F"
    },
    {
      "Title": "All Pro Sailboards / Kiteboards",
      "Fields": "Name,Nation,Craft Type,Course,Start Time,Speed (kts)",
      "Filter": "Craft Type<>Boat,Status=Pro Fleet"
    },
    {
      "Title": "All Gold Fleet Sailboards / Kiteboards",
      "Fields": "Name,Nation,Craft Type,Course,Start Time,Speed (kts)",
      "Filter": "Craft Type<>Boat,Status=Gold Fleet"
    },
    {
      "Title": "All Gold Fleet Sailboards",
      "Fields": "Name,Nation,Course,Start Time,Speed (kts)",
      "Filter": "Craft Type=Sailboard,Status=Gold Fleet"
    },
    {
      "Title": "All Amateur Sailboards - Masters",
      "Fields": "Name,Nation,Course,Start Time,Speed (kts)",
      "Filter": "Craft Type=Sailboard,Status=Amateur,Novice<>Y,Master=Y"
    },
    {
      "Title": "All Amateur Sailboards - Non-Masters",
      "Fields": "Name,Nation,Course,Start Time,Speed (kts)",
      "Filter": "Craft Type=Sailboard,Status=Amateur,Novice<>Y,Master<>Y"
    },
    {
      "Title": "All Novice Sailboards / Kiteboards",
      "Fields": "Name,Nation,Craft Type,Course,Start Time,Speed (kts)",
      "Filter": "Craft Type<>Boat,Novice=Y"
    },
    {
      "Title": "All Lady Sailboards",
      "Comment": "(excluding professionals)",
      "Fields": "Name,Nation,Status,Course,Start Time,Speed (kts)",
      "Filter": "Craft Type=Sailboard,Status<>Pro Fleet,Gender=F"
    },
    {
      "Title": "All Youth Sailboards",
      "Comment": "(age <17 on the first day of speed week)",
      "Fields": "Name,Nation,Course,Start Time,Speed (kts)",
      "Filter": "Craft Type=Sailboard,Status=Youth Fleet",
      "Period": "Session"
    },
    {
      "Title": "All Youth Sailboards",
      "Comment": "(age <17 on the first day of speed week)",
      "Fields": "Name,Nation,Course,Start Time,Speed (kts)",
      "Filter": "Craft Type=Sailboard,Status=Youth Fleet,Weekend=Y",
      "Sort": "Speed (kts)=DSC",
      "Period": "Event"
    }
  ]
}
