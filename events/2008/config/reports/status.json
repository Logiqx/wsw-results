{
  "Periods": [
    {
      "Suptitle": "Day Rankings by Status - {Date}",
      "Period": "Session"
    },
    {
      "Suptitle": "Overall Rankings by Status",
      "Period": "Event",
      "HideFields": "Course,Finish Time"
    }
  ],
  "Profile": true,
  "Reports": [
    {
      "Title": "Top Pros",
      "Fields": "Name,Nation,Sail Number,Craft Type,Course,Finish Time,Speed (kts)",
      "Filter": "Status=Pro Fleet",
      "Period": "Event"
    },
    {
      "Title": "Top Gold Fleet Entrants",
      "Fields": "Name,Nation,Sail Number,Craft Type,Course,Finish Time,Speed (kts)",
      "Filter": "Status=Gold Fleet",
      "Period": "Event"
    },
    {
      "Title": "Top Amateurs",
      "Fields": "Name,Nation,Sail Number,Craft Type,Course,Finish Time,Speed (kts)",
      "Filter": "Status=Amateur",
      "Period": "Event"
    },
    {
      "Title": "Top Novices",
      "Fields": "Name,Nation,Sail Number,Craft Type,Course,Finish Time,Speed (kts)",
      "Filter": "Novice=Y",
      "Period": "Event"
    },
    {
      "Title": "All Ladies",
      "Fields": "Name,Nation,Sail Number,Craft Type,Course,Finish Time,Speed (kts)",
      "Filter": "Gender=F"
    },
    {
      "Title": "Top Youths",
      "Comment": "(age <17 on the first day of speed week, weekend results only)",
      "Fields": "Name,Nation,Sail Number,Craft Type,Course,Finish Time,Speed (kts)",
      "Filter": "Status=Youth Fleet,Weekend=Y",
      "Sort": "Speed (kts)=DSC",
      "Period": "Event"
    },

    {
      "Title": "Top Masters",
      "Fields": "Name,Nation,Sail Number,Craft Type,Course,Finish Time,Speed (kts)",
      "Comment": "(age 45+)",
      "Filter": "Master=Y",
      "Period": "Event"
    }
  ]
}
