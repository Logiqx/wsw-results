{
  "Periods": [
    {
      "Suptitle": "Fastest of the Day - {Date}",
      "Period": "Session"
    },
    {
      "Suptitle": "Fastest of the Week",
      "Period": "Event",
      "HideFields": "Course,Time"
    }
  ],
  "Limit": 3,
  "Profile": true,
  "Reports": [
    {
      "Title": "Top 10",
      "Fields": "Name,Nation,Sail Number,Craft Type,Status,Course,Time,Speed (kts)",
      "Limit": 10
    },
    {
      "Title": "Fastest Boats",
      "Fields": "Name,Nation,Sail Number,Boat Name,Course,Time,Speed (kts)",
      "Filter": "Craft Type=Boat"
    },
    {
      "Title": "Fastest Kiteboards",
      "Fields": "Name,Nation,Sail Number,Status,Course,Time,Speed (kts)",
      "Filter": "Craft Type=Kiteboard"
    },
    {
      "Title": "Fastest Sailboards",
      "Fields": "Name,Nation,Sail Number,Status,Course,Time,Speed (kts)",
      "Filter": "Craft Type=Sailboard"
    },
    {
      "Title": "Fastest Craft TBCs",
      "Fields": "Name,Nation,Sail Number,Craft Type,Status,Course,Time,Speed (kts)",
      "Filter": "Craft Type=TBC"
    },
    {
      "Title": "Fastest Pro Sailboards",
      "Fields": "Name,Nation,Sail Number,Course,Time,Speed (kts)",
      "Filter": "Craft Type=Sailboard,Status=Pro Fleet"
    },
    {
      "Title": "Fastest Amateur Sailboards",
      "Fields": "Name,Nation,Sail Number,Course,Time,Speed (kts)",
      "Filter": "Craft Type=Sailboard,Status=Amateur"
    },
    {
      "Title": "Fastest Unclassified Sailboards",
      "Fields": "Name,Nation,Sail Number,Status,Course,Time,Speed (kts)",
      "Filter": "Craft Type=Sailboard,Status=TBC"
    },
    {
      "Title": "Fastest Novices",
      "Fields": "Name,Nation,Sail Number,Craft Type,Course,Time,Speed (kts)",
      "Filter": "Novice=Y"
    },
    {
      "Title": "Fastest Ladies",
      "Fields": "Name,Nation,Sail Number,Craft Type,Status,Course,Time,Speed (kts)",
      "Filter": "Gender=F"
    },
    {
      "Title": "Fastest Youths",
      "Comment": "(age <18 on 31st Oct, weekend only)",
      "Fields": "Name,Nation,Sail Number,Craft Type,Course,Time,Speed (kts)",
      "Filter": "Status=Youth Fleet"
    },
    {
      "Title": "Fastest Masters",
      "Fields": "Name,Nation,Sail Number,Craft Type,Status,Course,Time,Speed (kts)",
      "Comment": "(age 45+)",
      "Filter": "Master=Y"
    }
  ]
}
